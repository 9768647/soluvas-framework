imageConfigs(imageRepos) ::= <<
/**
 * Soluvas Image repositories configuration for JavaScript clients.
 */
define(['uritemplate'], function(UriTemplate) {

var imageConfigs = {
~imageRepos:imageRepo()~
};

return imageConfigs;
});
>>

imageRepo(repo) ::= <<
	'~repo.namespace~': {
		hiUriTemplate: '~repo.hiUriTemplate~',
		hiUriExpander: UriTemplate.parse('~repo.hiUriTemplate~'),
		loUriTemplate: '~repo.loUriTemplate~',
		loUriExpander: UriTemplate.parse('~repo.loUriTemplate~'),
	},

>>
