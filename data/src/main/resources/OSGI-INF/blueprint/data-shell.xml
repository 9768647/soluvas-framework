<blueprint xmlns="http://www.osgi.org/xmlns/blueprint/v1.0.0"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:schemaLocation="http://www.osgi.org/xmlns/blueprint/v1.0.0 http://www.osgi.org/xmlns/blueprint/v1.0.0
        http://karaf.apache.org/xmlns/shell/v1.1.0 http://karaf.apache.org/xmlns/shell/v1.1.0">

	<command-bundle xmlns="http://karaf.apache.org/xmlns/shell/v1.1.0">
		<!-- Kind -->
		<command name="kind/ls">
			<action class="org.soluvas.data.shell.KindLsCommand" />
		</command>
		
		<!-- Mixin -->
		<command name="mixin/ls">
			<action class="org.soluvas.data.shell.MixinLsCommand" />
		</command>
		
		<!-- Term -->
		<command name="term/ls">
			<action class="org.soluvas.data.shell.TermLsCommand" />
		</command>
		<command name="term/kinds">
			<action class="org.soluvas.data.shell.TermKindsCommand" />
		</command>
		
		<!-- Assoc -->
		<command name="assoc/ns">
			<action class="org.soluvas.data.shell.AssocNsCommand">
				<argument ref="svcLookup"/>
			</action>
		</command>
		<command name="assoc/count">
			<action class="org.soluvas.data.shell.AssocCountCommand">
				<argument ref="svcLookup"/>
			</action>
		</command>
		<command name="assoc/empty">
			<action class="org.soluvas.data.shell.AssocEmptyCommand">
				<argument ref="svcLookup"/>
			</action>
		</command>
		<command name="assoc/rmall">
			<action class="org.soluvas.data.shell.AssocRmAllCommand">
				<argument ref="svcLookup"/>
			</action>
		</command>
		<command name="assoc/ls">
			<action class="org.soluvas.data.shell.AssocLsCommand">
				<argument ref="svcLookup"/>
			</action>
		</command>
		<command name="assoc/add">
			<action class="org.soluvas.data.shell.AssocAddCommand">
				<argument ref="svcLookup"/>
			</action>
		</command>
		<command name="assoc/addinv">
			<action class="org.soluvas.data.shell.AssocAddInvCommand">
				<argument ref="svcLookup"/>
			</action>
		</command>
		<command name="assoc/rm">
			<action class="org.soluvas.data.shell.AssocRmCommand">
				<argument ref="svcLookup"/>
			</action>
		</command>
		<command name="assoc/rminv">
			<action class="org.soluvas.data.shell.AssocRmInvCommand">
				<argument ref="svcLookup"/>
			</action>
		</command>
		<command name="assoc/lefts">
			<action class="org.soluvas.data.shell.AssocLeftsCommand">
				<argument ref="svcLookup"/>
			</action>
		</command>
		<command name="assoc/left">
			<action class="org.soluvas.data.shell.AssocLeftCommand">
				<argument ref="svcLookup"/>
			</action>
		</command>
		<command name="assoc/rights">
			<action class="org.soluvas.data.shell.AssocRightsCommand">
				<argument ref="svcLookup"/>
			</action>
		</command>
		<command name="assoc/right">
			<action class="org.soluvas.data.shell.AssocRightCommand">
				<argument ref="svcLookup"/>
			</action>
		</command>
		
	</command-bundle>

</blueprint>
