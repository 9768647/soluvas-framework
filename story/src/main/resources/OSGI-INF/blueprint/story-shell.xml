<blueprint xmlns="http://www.osgi.org/xmlns/blueprint/v1.0.0"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:schemaLocation="http://www.osgi.org/xmlns/blueprint/v1.0.0 http://www.osgi.org/xmlns/blueprint/v1.0.0/blueprint.xsd
        http://karaf.apache.org/xmlns/shell/v1.1.0 http://karaf.apache.org/xmlns/shell/v1.1.0">

	<command-bundle xmlns="http://karaf.apache.org/xmlns/shell/v1.1.0">
	
		<!-- Story Schema -->
		<command name="storys/actionls">
			<action class="com.soluvas.story.shell.StorySActionLsCommand" />
		</command>
		<command name="storys/targetls">
			<action class="org.soluvas.social.shell.hand.StorySTargetLsCommand" />
		</command>
		<command name="storys/aggls">
			<action class="com.soluvas.story.shell.StorySAggLsCommand" />
		</command>
		
		<!-- Story -->
		<command name="story/add">
			<action class="com.soluvas.story.shell.StoryAddCommand" />
		</command>
		<command name="story/cat">
			<action class="com.soluvas.story.shell.StoryCatCommand" />
		</command>
		<command name="story/info">
			<action class="com.soluvas.story.shell.StoryInfoCommand" />
		</command>
		<command name="story/ls">
			<action class="com.soluvas.story.shell.StoryLsCommand" />
		</command>
		<command name="story/rm">
			<action class="com.soluvas.story.shell.StoryRmCommand" />
		</command>
		
	</command-bundle>

</blueprint>
