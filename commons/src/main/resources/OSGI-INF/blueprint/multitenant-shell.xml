<blueprint xmlns="http://www.osgi.org/xmlns/blueprint/v1.0.0"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:schemaLocation="http://www.osgi.org/xmlns/blueprint/v1.0.0 http://www.osgi.org/xmlns/blueprint/v1.0.0
        http://karaf.apache.org/xmlns/shell/v1.1.0 http://karaf.apache.org/xmlns/shell/v1.1.0">

	<command-bundle xmlns="http://karaf.apache.org/xmlns/shell/v1.1.0">
		<!-- Multitenant -->
		<command name="tenant/info">
			<action class="org.soluvas.commons.shell.TenantInfoCommand"/>
		</command>
		<command name="tenant/use">
			<action class="org.soluvas.commons.shell.TenantUseCommand"/>
		</command>
		<command name="tenant/env">
			<action class="org.soluvas.commons.shell.TenantEnvCommand"/>
		</command>
	</command-bundle>

</blueprint>
