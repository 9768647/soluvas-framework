      <xsd:documentation>Inspired by {@link org.eclipse.emf.common.notify.Notification}.</xsd:documentation>
    </xsd:annotation>
  </xsd:element>
  <xsd:complexType name="AttributeSet">
    <xsd:complexContent>
      <xsd:extension base="commons:AttributeNotification"/>
