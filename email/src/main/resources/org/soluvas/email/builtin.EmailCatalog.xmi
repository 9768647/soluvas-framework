<?xml version="1.0" encoding="ASCII"?>
<email:EmailCatalog
    xmi:version="2.0"
    xmlns:xmi="http://www.omg.org/XMI"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xmlns:email="http://www.soluvas.org/schema/email/1.0"
    xsi:schemaLocation="http://www.soluvas.org/schema/email/1.0 email.ecore"
    nsPrefix="builtin"
    generatedPackageName="org.soluvas.email.builtin">
  <pageTypes
      name="feedbackToAdminManager"
      subjectTemplate="[{{{appManifest.title}}}] Feedback from {{{contact.name}}}: {{{contact.subject}}}"/>
  <senderTypes
      name="general"
      from="{{{appManifest.title}}}"
      email="info@{{{appManifest.domain}}}"/>
  <senderTypes
      name="notification"
      from="{{{appManifest.title}}}"
      email="notification@{{{appManifest.domain}}}"/>
  <senderTypes
      name="trackedNotification"
      from="{{{appManifest.title}}}"
      email="notification+{{{tracker.code}}}@{{{appManifest.domain}}}"/>
  <senderTypes
      name="support"
      from="{{{appManifest.title}}} Support"
      email="support@{{{appManifest.domain}}}"/>
</email:EmailCatalog>
